{
  "name": "rpg-helper",
  "version": "0.3.0",
  "main": "dist/server/index.js",
  "repository": "https://github.com/kacpak/rpg-helper.git",
  "author": "Mateusz Kasprzak <kacpak@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=production node dist/server",
    "clean": "rimraf dist",
    "dev": "yarn run clean && run-p -c dev:*",
    "dev:compile-server": "babel src --out-dir dist --ignore app --watch --source-maps inline",
    "dev:run-server": "wait-on dist/server/index.js && nodemon dist/server -w dist -e js -i dist/public",
    "build": "cross-env NODE_ENV=production run-s clean build:*",
    "build:compile-server": "babel src --out-dir dist --ignore app",
    "build:compile-app": "webpack --config dist/server/config/webpack.config.js",
    "test": "run-s test:*",
    "test:lint": "eslint src",

    "heroku-postbuild": "yarn run build",
    "version": "yarn run build",
    "full-publish": "np"
  },
  "preferGlobal": true,
  "engines": {
    "node": "^7.10.0"
  },
  "bin": {
    "rpg-helper": "./cli.js"
  },
  "files": [
    "dist",
    "cli.js",
    "yarn.lock"
  ],
  "dependencies": {
    "bcrypt": "^1.0.2",
    "body-parser": "^1.17.2",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.3",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "express-history-api-fallback": "^2.2.1",
    "knex": "0.12.9",
    "lodash.omit": "^4.5.0",
    "mkdirp": "^0.5.1",
    "moment": "^2.18.1",
    "morgan": "^1.8.1",
    "objection": "^0.8.1",
    "passport": "^0.3.2",
    "passport-jwt": "^2.2.1",
    "socket.io": "^2.0.1",
    "socketio-jwt": "^4.5.0",
    "spdy": "^3.4.7",
    "split": "^1.0.0",
    "sqlite3": "^3.1.8",
    "winston": "^2.3.1",
    "yargs": "^8.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-dynamic-import-node": "^1.0.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.5.0",
    "babili-webpack-plugin": "^0.1.1",
    "bootstrap": "4.0.0-alpha.6",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.1",
    "eslint": "^3.19.0",
    "eslint-loader": "^1.7.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "node-sass": "^4.5.3",
    "nodemon": "^1.11.0",
    "np": "^2.16.0",
    "npm-run-all": "^4.0.2",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.18.1",
    "uglify-js": "git://github.com/mishoo/UglifyJS2#harmony-v2.8.22",
    "url-loader": "^0.5.8",
    "vee-validate": "^2.0.0-rc.5",
    "vue": "^2.3.3",
    "vue-loader": "^12.1.0",
    "vue-resource": "^1.3.1",
    "vue-router": "^2.5.3",
    "vue-template-compiler": "^2.3.3",
    "vuex": "^2.3.1",
    "vuex-router-sync": "^4.1.3",
    "wait-on": "^2.0.2",
    "webpack": "^2.5.1",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-merge": "^4.1.0"
  }
}
